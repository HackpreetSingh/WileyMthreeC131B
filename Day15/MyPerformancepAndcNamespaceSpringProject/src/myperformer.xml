<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="Keerthivasan" class="com.sujata.producer.Singer" p:song="Chennai Express">
	</bean>

	<bean id="Mahesh" class="com.sujata.producer.Dancer" c:style="Kuchipuri">
	</bean>

	<bean id="Madhav" class="com.sujata.producer.Juggler" c:balls="3">
	</bean>

	<bean id="guitar" class="com.sujata.producer.Guitar"></bean>
	<bean id="drum" class="com.sujata.producer.Drum"></bean>
	<bean id="tabla" class="com.sujata.producer.Tabla"></bean>

	<bean id="Deepika" class="com.sujata.producer.Instrumentalist" p:instrument-ref="guitar">
	</bean>

</beans>
